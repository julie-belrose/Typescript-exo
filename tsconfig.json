{
  "compilerOptions": {
    "rootDir": ".",
    "outDir": "dist",
    "module": "nodenext",
    "moduleResolution": "nodenext",
    "target": "esnext",
    "types": [
      "node"
    ],
    "strict": true,
    "noImplicitOverride": true,
    "noUncheckedIndexedAccess": true,
    "exactOptionalPropertyTypes": true
  },
  "include": [
    "exo-functions/src/**/*",
    "TP-typescript/src/**/*"
    //"exo-structures/src/**/*"
  ]
}

  
